#!/bin/sh
#
# usage: ./bin/deploy [APP]
#
#   APP defaults to carnival-staging
#
###
set -e

case "${1:-carnival-staging}" in
  carnival-staging) git push staging master ;;
  carnival-production) ./bin/copy-slug carnival-{staging,production} ;;
  *)
    sed '/^# \(usage:.*\)/!d; s//\1/' "$0" >&2
    exit 64
    ;;
esac
